{
  "name": "Compass Links",
  "version": "0.8.0",
  "description": "A popup menu with links to Compass related tools",

  "icons": { "16": "icon.png",
           "48": "icon med.png",
          "128": "icon lrg.png" },

  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
      "default_title": "Compass Links",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
